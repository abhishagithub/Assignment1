<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table id="loop" border="2px"></table>
    <script>
        var a = [
            {name:"abhsihek", id: 123, depart:"cse", salary: 10000},
            {name:"karan", id: 1234, depart:"ece", salary:120000 }
        ]
        let table = `<table>
            <tr>
            <th>name</th>
            <th>id</th>
            <th>depart</th>
            <th>salary</th>
            </tr>`
        a.forEach((abc) =>{
            table+=`<tr>
            <th>${abc.name}</th>
            <th>${abc.id}</th>
            <th>${abc.depart}</th>
            <th>${abc.salary}</th>
            </tr>`

        })
        document.getElementById("loop").innerHTML=table;
        

    </script>
</body>
</html>
